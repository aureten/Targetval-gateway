                { "type": "null" }
              ]
            }
          },
          {
            "name": "x-api-key",
            "in": "header",
            "required": false,
            "schema": {
              "title": "X-Api-Key",
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
