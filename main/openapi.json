    "/v1/targetval": {
      "get": {
        "summary": "Targetval",
        "operationId": "targetval_v1_targetval_get",
        "parameters": [
          {
            "name": "symbol",
            "in": "query",
            "required": false,
            "schema": {
              "title": "Symbol",
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ]
            }
          },
          {
            "name": "ensembl_id",
            "in": "query",
            "required": false,
            "schema": {
              "title": "Ensembl Id",
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ]
            }
          },
          {
            "name": "condition",
            "in": "query",
            "required": false,
            "schema": {
              "title": "Condition",
              "anyOf": [
                { "type": "string" },
                { "type": "null" }
              ]
            }
          },
          {
            "name": "efo_id",
            "in": "query",
            "required": false,
            "schema": {
              "title": "Efo Id",
