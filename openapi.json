  "components": {
    "schemas": {
      "RunRequest": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "gene_symbol": {
            "type": "string",
            "description": "HGNC symbol, e.g. IL23R"
          },
          "ensembl_id": {
            "type": "string",
            "description": "ENSG identifier"
          },
          "uniprot_id": {
            "type": "string",
            "description": "UniProt accession"
          },
          "trait_efo": {
            "type": "string",
            "description": "EFO ID for trait/condition"
          },
          "trait_label": {
            "type": "string",
            "description": "Human-readable trait label"
          },
          "rsid": {
            "type": "string",
            "description": "Lead variant rsID"
          },
          "region": {
            "type": "string",
            "description": "Genomic region (e.g. chr:pos-window)"
          },
          "run_questions": {
            "type": "array",
            "items": {
              "type": "integer",
              "minimum": 1,
              "maximum": 22
            }
          },
          "strict_human": {
            "type": "boolean",
            "default": true
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "description": "Optional limit for heavy modules"
          },
          "offset": {
            "type": "integer",
            "minimum": 0
          }
        }
      },
      "RunResponse": {
        "type": "object",
        "description": "Gateway run response containing per-question evidence and final recommendation.",
        "additionalProperties": true,
        "properties": {
          "context": {
            "type": "object"
          },
          "questions": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "final_recommendation": {
            "type": "object"
          }
        }
      },
      "ModuleRequest": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Module key to run"
          },
          "params": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "symbol": {
                "type": "string"
              },
              "gene": {
                "type": "string"
              },
              "ensembl_id": {
                "type": "string"
              },
              "uniprot_id": {
                "type": "string"
              },
              "efo": {
                "type": "string"
              },
              "condition": {
                "type": "string"
              },
              "tissue": {
                "type": "string"
              },
              "cell_type": {
                "type": "string"
              },
              "species": {
                "type": "string"
              },
              "limit": {
                "type": "integer"
              },
              "offset": {
                "type": "integer"
              },
              "strict": {
                "type": "boolean"
              }
            }
          }
        }
      },
      "ModuleResponse": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "module": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "records": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "edges": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "provenance": {
            "type": "object"
          }
        }
      },
      "HTTPValidationError": {
        "type": "object",
        "properties": {
          "detail": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationErrorItem"
            }
          }
        },
        "title": "HTTPValidationError"
      },
      "ValidationErrorItem": {
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "properties": {
          "loc": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "title": "ValidationError"
      }
    },
    "responses": {
      "BadRequest": {
        "description": "Bad Request",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ModuleResponse"
            }
          }
        }
      },
      "ValidationError": {
        "description": "Validation error",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/HTTPValidationError"
            }
          }
        }
      },
      "ServerError": {
        "description": "Server error",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "detail": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
