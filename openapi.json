    "/aggregate": {
      "post": {
        "operationId": "aggregate",
        "summary": "Aggregate evidence modules (RAW) or return anchor-based SCORE report",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "gene": {
                    "type": "string"
                  },
                  "condition": {
                    "type": "string"
                  },
                  "efo": {
                    "type": "string"
                  },
                  "limit": {
                    "type": "integer"
                  },
                  "mode": {
                    "type": "string",
                    "enum": [
                      "RAW",
                      "SCORE"
                    ],
                    "default": "RAW"
                  },
                  "include_evidence": {
                    "type": "boolean",
                    "default": true
                  }
                },
                "required": [
                  "gene",
                  "condition"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "RAW mode -> AggregateResponse; SCORE mode -> ScoreReport",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/AggregateResponse"
                    },
                    {
                      "$ref": "#/components/schemas/ScoreReport"
                    }
                  ]
                }
              }
            }
          }
        },
        "description": "Mode RAW: returns raw evidence per module (module_id -> Evidence). Mode SCORE: runs modules and returns bucket-level anchor scores, final_score, and confidence."
      }
