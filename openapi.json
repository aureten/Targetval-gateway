{
  "openapi": "3.1.0",
  "info": {
    "title": "TargetVal Gateway (Aggregate + Modules + Scoring)",
    "version": "1.3.3",
    "description": "OpenAPI spec exposing 29 module endpoints, /aggregate for raw evidence, and a new /score endpoint that returns bucket-level, anchor-based scores and the final TargetVal score. Compatible with ChatGPT Actions. Consolidated /comp endpoints into a single /comp with 'metric' query parameter. Fixed inline parameters and AggregateResponse.properties for strict Actions validators."
  },
  "servers": [
    {
      "url": "https://targetval-gateway.onrender.com"
    }
  ],
  "components": {
    "schemas": {
      "Evidence": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "OK",
              "ERROR",
              "NO_DATA"
            ]
          },
          "source": {
            "type": "string"
          },
          "fetched_n": {
            "type": "integer"
          },
          "data": {
            "type": "object",
            "additionalProperties": true
          },
          "citations": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "fetched_at": {
            "type": "number"
          }
        },
        "required": [
          "status"
        ]
      },
      "AggregateResponse": {
        "type": "object",
        "description": "Map of module_id -> Evidence",
        "properties": {},
        "additionalProperties": {
          "$ref": "#/components/schemas/Evidence"
        }
      },
      "BucketScore": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "enum": [
              "B1",
              "B2",
              "B3",
              "B4",
              "B5",
              "B6",
              "B7"
            ]
          },
          "name": {
            "type": "string"
          },
          "score": {
            "oneOf": [
              {
                "type": "number",
                "minimum": 0.0,
                "maximum": 1.0
              },
              {
                "type": "null"
              }
            ],
            "description": "0\u20131 for B1\u2013B6; null for B7 (qualitative only)."
          },
          "notes": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Anchor hits, adjustments, qualitative comments."
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "ScoreReport": {
        "type": "object",
        "properties": {
          "gene": {
            "type": "string"
          },
          "condition": {
            "type": "string"
          },
          "efo": {
            "type": "string"
          },
          "final_score": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0
          },
          "confidence": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0
          },
          "bucket_scores": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BucketScore"
            }
          },
          "positives": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "negatives": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "evidence": {
            "$ref": "#/components/schemas/AggregateResponse"
          }
        },
        "required": [
          "gene",
          "condition",
          "final_score",
          "bucket_scores"
        ]
      }
    },
    "parameters": {
      "GeneParam": {
        "in": "query",
        "name": "gene",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "ConditionParam": {
        "in": "query",
        "name": "condition",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "LimitParam": {
        "in": "query",
        "name": "limit",
        "schema": {
          "type": "integer",
          "default": 50,
          "minimum": 1,
          "maximum": 500
        }
      },
      "CompMetricParam": {
        "in": "query",
        "name": "metric",
        "required": true,
        "schema": {
          "type": "string",
          "enum": [
            "intensity",
            "freedom"
          ]
        },
        "description": "Choose 'intensity' (competition intensity) or 'freedom' (freedom-to-operate). Qualitative only; not numerically scored."
      }
    }
  },
