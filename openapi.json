
  "openapi": "3.1.0",
  "info": {
    "title": "TargetVal Gateway API",
    "version": "2.0.0",
    "description": "Public OpenAPI aligned to the v2.0 router/main refresh (live-first).\nEndpoints reflect the six core buckets (B1â€“B6), Synthesis, Literature, and Utility.\nEvidence envelopes remain backward compatible and include citations to all upstream calls."
  },
  "tags": [
    {
      "name": "Utility"
    },
    {
      "name": "Synthesis"
    },
    {
      "name": "Literature"
    },
    {
      "name": "B1 Genetics & Causality"
    },
    {
      "name": "B2 Assoc & Perturbation"
    },
    {
      "name": "B3 Expression & Localization"
    },
    {
      "name": "B4 Mechanistic Wiring"
    },
    {
      "name": "B5 Tractability & Modality"
    },
    {
      "name": "B6 Clinical & Safety"
    },
    {
      "name": "B7 Competition & IP"
    }
  ],
  "servers": [
    {
      "url": "https://targetval-gateway.onrender.com"
    }
  ],
  "components": {
    "schemas": {
      "Evidence": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "enum": [
              "OK",
              "ERROR",
              "NO_DATA"
            ]
          },
          "source": {
            "type": "string"
          },
          "fetched_n": {
            "type": "integer"
          },
          "data": {
            "type": "object",
            "additionalProperties": true
          },
          "citations": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "fetched_at": {
            "type": "number"
          },
          "error": {
            "type": "string"
          }
        },
        "required": [
          "status"
        ]
      },
      "AggregateEnvelope": {
        "type": "object",
        "properties": {
          "query": {
            "type": "object",
            "properties": {
              "gene": {
                "type": "string"
              },
              "symbol": {
                "type": "string"
              },
              "ensembl_id": {
                "type": "string"
              },
              "efo": {
                "type": "string"
              },
              "condition": {
                "type": "string"
              },
              "limit": {
                "type": "integer"
              },
              "modules": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "species": {
                "type": "integer"
              },
              "cutoff": {
                "type": "number"
              },
              "mode": {
                "type": "string",
                "enum": [
                  "auto",
                  "live",
                  "snapshot"
                ]
              }
            }
          },
          "results": {
            "type": "object",
            "additionalProperties": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "enum": [
                    "OK",
                    "ERROR",
                    "NO_DATA"
                  ]
                },
                "source": {
                  "type": "string"
                },
                "fetched_n": {
                  "type": "integer"
                },
                "data": {
                  "type": "object",
                  "additionalProperties": true
                },
                "citations": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "fetched_at": {
                  "type": "number"
                },
                "error": {
                  "type": "string"
                }
              },
              "required": [
                "status"
              ]
            }
          }
        },
        "required": [
          "results"
        ]
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      }
    }
  },
  "paths": {
    "/healthz": {
      "get": {
        "summary": "Health check",
        "tags": [
          "Utility"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/": {
