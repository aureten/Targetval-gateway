                      "type": "string"
                    }
                  },
                  "species": {
                    "type": "integer"
                  },
                  "cutoff": {
                    "type": "number"
                  },
                  "mode": {
                    "type": "string",
                    "enum": [
                      "auto",
                      "live",
                      "snapshot"
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AggregateEnvelope"
                }
              }
            }
          }
